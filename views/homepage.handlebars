<!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<div class="row mb-4 project">
  <div class="col-md-12">
    <h2>
      <a href="/post/{{post.id}}">{{post.title}}</a>
    </h2>
    <p class="text-link">Created on {{format_date post.date_created}}</p>
  </div>
</div>
{{/each}}
